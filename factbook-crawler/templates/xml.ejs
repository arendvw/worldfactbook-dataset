			<assessment><%= getField(doc,'communications','Telephone system','general assessment:') %></assessment>
			<domestic><%= getField(doc,'communications','Telephone system','domestic:') %></domestic>
			<international><%= getField(doc,'communications','Telephone system','international:') %></international>
